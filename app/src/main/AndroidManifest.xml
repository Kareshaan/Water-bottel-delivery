<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.waterdelivery2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.WaterDelivery2">
        <activity android:name=".delivery.delivery_confirmation"></activity>
        <activity
            android:name=".drawer"
            android:label="@string/title_activity_drawer"
            android:theme="@style/Theme.WaterDelivery2.NoActionBar"></activity>
        <activity android:name=".payment.payment" />
        <activity android:name=".payment.payment_option" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.WaterDelivery2.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>